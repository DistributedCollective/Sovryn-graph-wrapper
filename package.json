{
   "name": "sov-graph-wrapper-service",
   "version": "1.0.0",
   "description": "Sovryn graph wrapper Service",
   "main": "app.js",
   "scripts": {
      "build": "tsc",
      "start": "node ./dist/server.js",
      "clean": "rm -rf ./dist && mkdir dist",
      "dev": "nodemon --config nodemon.json src/server.ts",
      "dev:debug": "nodemon --config nodemon.json --inspect-brk src/server.ts",
      "test": "jest",
      "test:watch": "jest --watchAll",
      "lint": "ts-standard",
      "lint:fix": "ts-standard --fix",
      "migrations:generate": "npx ts-node ./node_modules/.bin/typeorm migration:generate -f ./src/config/database.ts",
      "migrations:run": "npx ts-node ./node_modules/.bin/typeorm migration:run -f ./src/config/database.ts",
      "migrations:revert": "npx ts-node ./node_modules/.bin/typeorm migration:revert -f ./src/config/database.ts",
      "prepare": "husky install",
      "codegen": "graphql-codegen --config ./codegen.yml",
      "migrate:dev": "export POSTGRES_HOST=localhost && export POSTGRES_PASSWORD=postgres export POSTGRES_DB=sov-graph-wrapper-main && npm run migrations:run"
   },
   "keywords": [
      "Sovryn"
   ],
   "author": "Remy",
   "license": "ISC",
   "dependencies": {
      "@apollo/client": "^3.6.9",
      "@blobfishkate/sovryncontractswip": "^0.2.15",
      "apollo-cache-inmemory": "^1.6.6",
      "apollo-client": "^2.6.10",
      "apollo-link-http": "^1.5.17",
      "class-validator": "^0.13.2",
      "convict": "^6.2.3",
      "convict-format-with-validator": "^6.2.0",
      "cors": "^2.8.5",
      "cron": "^1.8.2",
      "cross-fetch": "^3.1.5",
      "express": "^4.18.1",
      "express-request-id": "^1.4.1",
      "express-validator": "^6.14.2",
      "graphql": "^16.5.0",
      "graphql-tag": "^2.12.6",
      "helmet": "^4.6.0",
      "lodash": "^4.17.21",
      "mathjs": "^10.6.4",
      "moment": "^2.29.4",
      "node-fetch": "^3.2.10",
      "npm": "^8.15.1",
      "pg": "^8.7.3",
      "pino": "^6.14.0",
      "pino-http": "^5.8.0",
      "pino-pretty": "^5.1.3",
      "reflect-metadata": "^0.1.13",
      "regenerator-runtime": "^0.13.7",
      "response-time": "^2.3.2",
      "typeorm": "^0.2.45",
      "uuid": "^8.3.1",
      "web3": "^1.7.5",
      "web3-utils": "^1.7.5"
   },
   "devDependencies": {
      "@babel/cli": "^7.18.10",
      "@babel/core": "^7.18.10",
      "@babel/node": "^7.18.10",
      "@babel/preset-env": "^7.18.10",
      "@graphql-codegen/cli": "2.6.2",
      "@graphql-codegen/introspection": "2.1.1",
      "@graphql-codegen/typescript": "2.4.8",
      "@graphql-codegen/typescript-document-nodes": "2.2.8",
      "@graphql-codegen/typescript-resolvers": "2.6.1",
      "@tsconfig/node12": "^1.0.11",
      "@types/bunyan": "^1.8.8",
      "@types/convict": "^6.1.1",
      "@types/convict-format-with-validator": "^6.0.2",
      "@types/cors": "^2.8.12",
      "@types/cron": "^1.7.3",
      "@types/express": "^4.17.13",
      "@types/express-request-id": "^1.4.3",
      "@types/graphql": "^14.5.0",
      "@types/jest": "^27.5.2",
      "@types/lodash": "^4.14.182",
      "@types/node": "^8.10.66",
      "@types/pino": "^6.3.12",
      "@types/pino-http": "^5.8.1",
      "@types/response-time": "^2.3.5",
      "@types/supertest": "^2.0.12",
      "apollo": "^2.34.0",
      "husky": "^7.0.4",
      "jest": "^27.5.1",
      "nodemon": "^2.0.19",
      "supertest": "^6.2.4",
      "ts-jest": "^27.1.5",
      "ts-node": "^10.9.1",
      "ts-standard": "^10.0.0",
      "typechain": "^8.1.0",
      "typescript": "^4.7.4"
   }
}
